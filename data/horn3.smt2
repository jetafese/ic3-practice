(declare-rel Invariant (Bool Bool Bool))
(declare-rel Goal ())
(declare-var x Bool)
(declare-var y Bool)
(declare-var z Bool)
(declare-var xp Bool)
(declare-var yp Bool)
(declare-var zp Bool)
(rule (=> (not (or x y z)) (Invariant x y z)))
(rule (=> (and (Invariant x y z)
  (= (and x y z) zp)
  ) (Invariant xp yp zp)))
(rule (=> (and (Invariant x y z) z) Goal))
(query Goal)
